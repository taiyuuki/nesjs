  "main": "./dist/nesjs-vue.umd.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/taiyuuki/nesjs"
  },
  "bugs": "https://github.com/taiyuuki/nesjs/issues",
